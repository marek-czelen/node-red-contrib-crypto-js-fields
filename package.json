{
  "name": "node-red-contrib-crypto-js-field",
  "version": "0.1.1",
  "description": "Node-RED nodes using CryptoJS to encrypt and decrypt messages",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marek-czelen/node-red-contrib-crypto-js-fields.git"
  },
  "scripts": {
    "test": "node-red"
  },
  "keywords": [
    "node-red"
  ],
  "author": "Bernardo Baumblatt / Marek Czelen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marek-czelen/node-red-contrib-crypto-js-fields/issues"
  },
  "homepage": "https://github.com/marek-czelen/node-red-contrib-crypto-js-fields#readme",
  "dependencies": {
    "crypto-js": "^3.1.9-1"
  },
  "node-red": {
    "nodes": {
      "encrypt": "cryptojs/encrypt.js",
      "decrypt": "cryptojs/decrypt.js",
      "hmac": "cryptojs/hmac.js",
      "digest": "cryptojs/digest.js",
      "encode": "cryptojs/encode.js",
      "decode": "cryptojs/decode.js"
    }
  }
}
